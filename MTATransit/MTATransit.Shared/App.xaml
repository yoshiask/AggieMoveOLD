<Application
    x:Class="MTATransit.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MTATransit"
    RequestedTheme="Light">

    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="DINFontFamily">/Assets/Fonts/DINRegular#DIN</FontFamily>
            <FontFamily x:Key="LAMoveIconFont">/Assets/Fonts/LA-Move-Icons#LA-Move-Icons</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
